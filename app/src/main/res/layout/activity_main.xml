<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/activity_main"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="com.example.mannem.project17.MainActivity">

    <TextView
        android:id="@+id/typestring"
        android:layout_width="179dp"
        android:layout_height="390dp"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="19dp"
        android:textSize="24sp" />

    <Button
        android:id="@+id/sector1"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="0dp"
        android:text="one" />

    <Button
        android:id="@+id/sector2"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="0dp"
        android:text="two" />

    <Button
        android:id="@+id/sector3"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_below="@+id/typestring"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:layout_marginTop="-333dp"
        android:text="three" />

    <Button
        android:id="@+id/sector4"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_below="@+id/typestring"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="-333dp"
        android:layout_marginEnd="0dp"
        android:text="four" />

    <Button
        android:id="@+id/sector5"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_below="@+id/sector4"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:text="five" />

    <Button
        android:id="@+id/sector6"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_below="@+id/sector4"
        android:layout_alignParentEnd="true"
        android:layout_marginEnd="0dp"
        android:text="six" />

    <Button
        android:id="@+id/sector7"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_below="@+id/sector5"
        android:layout_alignParentStart="true"
        android:layout_marginStart="0dp"
        android:text="seven" />

    <Button
        android:id="@+id/sector8"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_below="@+id/sector5"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="2dp"
        android:layout_marginEnd="0dp"
        android:text="eight" />

    <Button
        android:id="@+id/sector9"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_below="@+id/sector7"
        android:layout_alignParentStart="true"
        android:text="nine" />

    <Button
        android:id="@+id/sector10"
        android:layout_width="76dp"
        android:layout_height="76dp"
        android:layout_below="@+id/sector8"
        android:layout_alignParentEnd="true"
        android:text="ten" />

    <Button
        android:id="@+id/settings"
        android:layout_width="75dp"
        android:layout_height="30dp"
        android:layout_alignParentBottom="true"
        android:layout_alignParentEnd="true"
        android:onClick="openScreen"
        android:text="Settings"
        android:textSize="8sp" />

    <Button
        android:id="@+id/collector"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBottom="@+id/settings"
        android:layout_centerHorizontal="true"
        android:text="Collect" />

</RelativeLayout>
